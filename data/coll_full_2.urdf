<?xml version="1.0" ?>
<robot name="maiskolben">
  <link name="baseLink">
     <contact>
      <lateral_friction value="1"/>
  </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="1.0"/>
       <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
    </inertial>
    
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
        <!-- <mesh filename="PingPongLaterne.obj" scale="0.01 0.01 0.01"/> -->
          <box size="120 120 10"/>
        </geometry>
       <material name="white">
        <color rgba="0 1 0 0.2"/>
      </material>
    </visual>

  <!-- <collision group="0" mask="0"> -->

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="120 120 10"/>
      </geometry>
    </collision>   

  </link>
</robot>

